{
  "name": "se-test-lkmx",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "build:prod": "next build",
    "start": "next start",
    "lint": "eslint",
    
    "db:up": "docker-compose up postgres -d",
    "db:down": "docker-compose down",
    "db:migrate": "npx prisma migrate deploy",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force",
    
    "dev:db": "powershell -ExecutionPolicy Bypass -File ./start-dev.ps1 && npm run dev",
    "dev:full": "powershell -ExecutionPolicy Bypass -File ./start-app.ps1",
    
    "docker:logs": "docker-compose logs -f",
    "docker:stop-app": "docker-compose stop nextjs-app"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "next": "15.5.2",
    "prisma": "^6.14.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}